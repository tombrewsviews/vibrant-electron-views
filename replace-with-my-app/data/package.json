{
  "name": "patient-portal-data",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "diagram": "cd diagram; ./make.sh",
    "hasura": "env-cmd -f .env hasura console",
    "hasura:staging": "env-cmd -f .env.staging hasura console",
    "hasura:production": "env-cmd -f .env.production hasura console",
    "migrate": "env-cmd -f .env hasura migrate",
    "release": "./release.sh",
    "setup": "yarn migrate apply && yarn metadata setup && yarn sample:data",
    "migrate:staging": "env-cmd -f .env.staging hasura migrate",
    "migrate:production": "env-cmd -f .env.production hasura migrate",
    "migrate:status": "env-cmd -f .env hasura migrate status",
    "metadata": "env-cmd -f .env node metadata.js",
    "presample:data": "yarn remove:sample:data",
    "sample:data": "env-cmd -f .env node sample-data/make.js",
    "remove:sample:data": "env-cmd -f .env node sample-data/remove.js",
    "design": "yarn metadata clear && yarn migrate apply --down all && yarn migrate apply && yarn metadata setup --onlyTrackTables",
    "start": "docker-compose pull && docker-compose up",
    "down": "docker-compose down"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^3.0.15",
    "aws-sdk": "^2.596.0",
    "bottleneck": "^2.19.5",
    "csv-parse": "^4.6.5",
    "date-fns": "^2.8.1",
    "dlv": "^1.1.3",
    "env-cmd": "^9.0.3",
    "faker": "^4.1.0",
    "fetchu": "^4.1.0",
    "libphonenumber-js": "^1.7.26",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.2",
    "uuid": "^3.3.3",
    "yaml": "^1.7.2",
    "zipcodes": "^8.0.0"
  }
}
